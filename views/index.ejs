<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pokemon Management</title>
</head>

<body>
    <h1>Trainers</h1>
    <ul>
        <% trainers.map((trainer)=> { %>
            <li>
                <p>
                    <%= trainer.name %>
                </p>
            </li>
            <% }) %>
    </ul>
    <h1>Pok√©mons</h1>
    <ul>
        <% pokemons.map((pokemon)=> { %>
            <li>
                <p>
                    <%= pokemon.name %>
                </p>
            </li>
            <% }) %>
    </ul>
    <h1>Types</h1>
    <ul>
        <% types.map((type)=> { %>
            <li>
                <p>
                    <%= type.name %>
                </p>
            </li>
            <% }) %>
    </ul>
</body>

</html>